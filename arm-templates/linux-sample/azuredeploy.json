{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "linuxvmstorage01dksType": {
      "type": "string",
      "defaultValue": "Standard_LRS",
      "allowedValues": [
        "Standard_LRS",
        "Standard_GRS",
        "Premium_LRS"
      ]
    },
    "linuxvm-1Name": {
      "type": "string",
      "minLength": 1
    },
    "linuxvm-1AdminUserName": {
      "type": "string",
      "minLength": 1
    },
    "linuxvm-1AdminPassword": {
      "type": "securestring"
    },
    "linuxvm-1UbuntuOSVersion": {
      "type": "string",
      "defaultValue": "14.04.2-LTS",
      "allowedValues": [
        "12.04.5-LTS",
        "14.04.2-LTS"
      ]
    },
    "linuxvm-2Name": {
      "type": "string",
      "minLength": 1
    },
    "linuxvm-2AdminUserName": {
      "type": "string",
      "minLength": 1
    },
    "linuxvm-2AdminPassword": {
      "type": "securestring"
    },
    "linuxvm-2UbuntuOSVersion": {
      "type": "string",
      "defaultValue": "14.04.2-LTS",
      "allowedValues": [
        "12.04.5-LTS",
        "14.04.2-LTS"
      ]
    },
    "database-asName": {
      "type": "string",
      "minLength": 1
    }
  },
  "variables": {
    "advarm-vnetPrefix": "10.1.0.0/24",
    "advarm-vnetSubnet1Name": "Apps",
    "advarm-vnetSubnet1Prefix": "10.1.0.0/25",
    "advarm-vnetSubnet2Name": "Data",
    "advarm-vnetSubnet2Prefix": "10.1.0.128/28",
    "advarm-vnetSubnet3Name": "Dmz",
    "advarm-vnetSubnet3Prefix": "10.1.0.144/28",
    "public-ipName": "public-ip",
    "linuxvmstorage01dksName": "[concat('linuxvmstorage01dks', uniqueString(resourceGroup().id))]",
    "linuxvm-1ImagePublisher": "Canonical",
    "linuxvm-1ImageOffer": "UbuntuServer",
    "linuxvm-1OSDiskName": "linuxvm-1OSDisk",
    "linuxvm-1VmSize": "Standard_D2_v2",
    "linuxvm-1VnetID": "[resourceId('Microsoft.Network/virtualNetworks', 'advarm-vnet')]",
    "linuxvm-1SubnetRef": "[concat(variables('linuxvm-1VnetID'), '/subnets/', variables('advarm-vnetSubnet2Name'))]",
    "linuxvm-1StorageAccountContainerName": "vhds",
    "linuxvm-1NicName": "[concat(parameters('linuxvm-1Name'), 'NetworkInterface')]",
    "linuxvm-2ImagePublisher": "Canonical",
    "linuxvm-2ImageOffer": "UbuntuServer",
    "linuxvm-2OSDiskName": "linuxvm-2OSDisk",
    "linuxvm-2VmSize": "Standard_D2_v2",
    "linuxvm-2VnetID": "[resourceId('Microsoft.Network/virtualNetworks', 'advarm-vnet')]",
    "linuxvm-2SubnetRef": "[concat(variables('linuxvm-2VnetID'), '/subnets/', variables('advarm-vnetSubnet2Name'))]",
    "linuxvm-2StorageAccountContainerName": "vhds",
    "linuxvm-2NicName": "[concat(parameters('linuxvm-2Name'), 'NetworkInterface')]"
  },
  "resources": [
    {
      "name": "advarm-vnet",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "advarm-vnet"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('advarm-vnetPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('advarm-vnetSubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('advarm-vnetSubnet1Prefix')]"
            }
          },
          {
            "name": "[variables('advarm-vnetSubnet2Name')]",
            "properties": {
              "addressPrefix": "[variables('advarm-vnetSubnet2Prefix')]"
            }
          },
          {
            "name": "[variables('advarm-vnetSubnet3Name')]",
            "properties": {
              "addressPrefix": "[variables('advarm-vnetSubnet3Prefix')]"
            }
          }
        ]
      }
    },
    {
      "name": "[variables('public-ipName')]",
      "type": "Microsoft.Network/publicIPAddresses",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "public-ip"
      },
      "properties": {
        "publicIPAllocationMethod": "Dynamic"
      }
    },
    {
      "name": "[variables('linuxvmstorage01dksName')]",
      "type": "Microsoft.Storage/storageAccounts",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-01-01",
      "sku": {
        "name": "[parameters('linuxvmstorage01dksType')]"
      },
      "dependsOn": [ ],
      "tags": {
        "displayName": "linuxvmstorage01dks"
      },
      "kind": "Storage"
    },
    {
      "name": "[variables('linuxvm-1NicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', 'advarm-vnet')]"
      ],
      "tags": {
        "displayName": "linuxvm-1Nic"
      },
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAllocationMethod": "Dynamic",
              "subnet": {
                "id": "[variables('linuxvm-1SubnetRef')]"
              }
            }
          }
        ]
      }
    },
    {
      "name": "[parameters('linuxvm-1Name')]",
      "type": "Microsoft.Compute/virtualMachines",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-06-15",
      "dependsOn": [
        "[resourceId('Microsoft.Storage/storageAccounts', variables('linuxvmstorage01dksName'))]",
        "[resourceId('Microsoft.Network/networkInterfaces', variables('linuxvm-1NicName'))]",
        "[resourceId('Microsoft.Compute/availabilitySets', parameters('database-asName'))]"
      ],
      "tags": {
        "displayName": "linuxvm-1"
      },
      "properties": {
        "availabilitySet": {
          "id": "[resourceId('Microsoft.Compute/availabilitySets', parameters('database-asName'))]"
        },
        "hardwareProfile": {
          "vmSize": "[variables('linuxvm-1VmSize')]"
        },
        "osProfile": {
          "computerName": "[parameters('linuxvm-1Name')]",
          "adminUsername": "[parameters('linuxvm-1AdminUsername')]",
          "adminPassword": "[parameters('linuxvm-1AdminPassword')]"
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "[variables('linuxvm-1ImagePublisher')]",
            "offer": "[variables('linuxvm-1ImageOffer')]",
            "sku": "[parameters('linuxvm-1UbuntuOSVersion')]",
            "version": "latest"
          },
          "osDisk": {
            "name": "linuxvm-1OSDisk",
            "vhd": {
              "uri": "[concat(reference(resourceId('Microsoft.Storage/storageAccounts', variables('linuxvmstorage01dksName')), '2016-01-01').primaryEndpoints.blob, variables('linuxvm-1StorageAccountContainerName'), '/', variables('linuxvm-1OSDiskName'), '.vhd')]"
            },
            "caching": "ReadWrite",
            "createOption": "FromImage"
          }
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('linuxvm-1NicName'))]"
            }
          ]
        }
      }
    },
    {
      "name": "[variables('linuxvm-2NicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', 'advarm-vnet')]"
      ],
      "tags": {
        "displayName": "linuxvm-2Nic"
      },
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAllocationMethod": "Dynamic",
              "subnet": {
                "id": "[variables('linuxvm-2SubnetRef')]"
              }
            }
          }
        ]
      }
    },
    {
      "name": "[parameters('linuxvm-2Name')]",
      "type": "Microsoft.Compute/virtualMachines",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-06-15",
      "dependsOn": [
        "[resourceId('Microsoft.Storage/storageAccounts', variables('linuxvmstorage01dksName'))]",
        "[resourceId('Microsoft.Network/networkInterfaces', variables('linuxvm-2NicName'))]",
        "[resourceId('Microsoft.Compute/availabilitySets', parameters('database-asName'))]"
      ],
      "tags": {
        "displayName": "linuxvm-2"
      },
      "properties": {
        "availabilitySet": {
          "id": "[resourceId('Microsoft.Compute/availabilitySets', parameters('database-asName'))]"
        },
        "hardwareProfile": {
          "vmSize": "[variables('linuxvm-2VmSize')]"
        },
        "osProfile": {
          "computerName": "[parameters('linuxvm-2Name')]",
          "adminUsername": "[parameters('linuxvm-2AdminUsername')]",
          "adminPassword": "[parameters('linuxvm-2AdminPassword')]"
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "[variables('linuxvm-2ImagePublisher')]",
            "offer": "[variables('linuxvm-2ImageOffer')]",
            "sku": "[parameters('linuxvm-2UbuntuOSVersion')]",
            "version": "latest"
          },
          "osDisk": {
            "name": "linuxvm-2OSDisk",
            "vhd": {
              "uri": "[concat(reference(resourceId('Microsoft.Storage/storageAccounts', variables('linuxvmstorage01dksName')), '2016-01-01').primaryEndpoints.blob, variables('linuxvm-2StorageAccountContainerName'), '/', variables('linuxvm-2OSDiskName'), '.vhd')]"
            },
            "caching": "ReadWrite",
            "createOption": "FromImage"
          }
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('linuxvm-2NicName'))]"
            }
          ]
        }
      }
    },
    {
      "name": "[parameters('database-asName')]",
      "type": "Microsoft.Compute/availabilitySets",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-06-15",
      "dependsOn": [ ],
      "tags": {
        "displayName": "database-as"
      },
      "properties": {
        "platformUpdateDomainCount": 5,
        "platformFaultDomainCount": 2
      }
    }
  ],
  "outputs": {
  }
}
